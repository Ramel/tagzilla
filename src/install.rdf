<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>{4eac6fec-f68b-4797-be7a-ffeea73e1495}</em:id>
    <em:version>0.066.3a1</em:version>

    <!-- Target Application this extension can install into,
        with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <!-- Firefox -->
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>0.7</em:minVersion>
        <em:maxVersion>8.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:targetApplication>
      <Description>
        <!-- Thunderbird -->
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>0.4</em:minVersion>
        <em:maxVersion>8.0</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:targetApplication>
      <Description>
        <!-- SeaMonkey -->
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>3.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <!-- For future expansion
    <em:targetApplication>
      <Description>
        <!- - Sunbird - ->
        <em:id>{718e30fb-e89b-41dd-9da7-e25a45638b28}</em:id>
        <em:minVersion>0.1</em:minVersion>
        <em:maxVersion>1.1</em:maxVersion>
      </Description>
    </em:targetApplication>
    -->

    <!-- Front End MetaData -->
    <em:name>TagZilla</em:name>
    <em:description>Tagline selection and manipulation tool</em:description>
    <em:creator>David Perry</em:creator>
    <em:homepageURL>http://tagzilla.mozdev.org/</em:homepageURL>

    <!-- Front End Integration Hooks (used by Extension Manager)-->
    <em:optionsURL>chrome://tagzilla/content/pref/tagzillaPrefDialog.xul</em:optionsURL>
    <em:aboutURL>chrome://tagzilla/content/about.xul</em:aboutURL>
    <em:iconURL>chrome://tagzilla/content/tagzilla-32.png</em:iconURL>

    <!-- Packages, Skins and Locales that this extension registers -->
    <em:file>
      <Description about="urn:mozilla:extension:file:tagzilla.jar">
        <em:package>tagzilla/content/</em:package>
        <em:locale>tagzilla/locale/en-US/</em:locale>
        <em:locale>tagzilla/locale/de-AT/</em:locale>
        <em:locale>tagzilla/locale/de-DE/</em:locale>
        <em:locale>tagzilla/locale/es-AR/</em:locale>
        <em:locale>tagzilla/locale/fr-FR/</em:locale>
        <em:locale>tagzilla/locale/ru-RU/</em:locale>
        <em:skin>tagzilla/skin/classic/</em:skin>
      </Description>
    </em:file>
  </Description>
</RDF>
